{
  "face": {
    "general": {
      "output": {
        "verbose": "FALSE",
        "video": "FALSE",
        "videoFourCC": "XVID",
        "videoFPS": "25"
      },
      "directories": {
        "faceDetector": "facedetector/",
        "shapeModel": "shapemodel/",
        "output": "output/"
      }
    },
    "modules": {
      "imageQueue": {
        "connection": [ "FaceDetection:1", "UserManager:1", "UserProcessor:1", "Visualizer:1" ],
        "bound": "10",
        "samplingFPS": "30.0",
        "thresholdMS": "100"
      },
      "faceDetection": {
        "connection": [ "UserManager:2" ],
        "detectionSec": "10.0",
        "detectionOverlap": "0.2",
        "detectMultiScale": {
          "flags": "0",
          "maxSize": "1.0",
          "minNeighbors": "3",
          "minSize": "0.05",
          "scaleFactor": "1.1"
        },
        "fileName": "haarcascade_frontalface_alt2.xml",
        "imageScale": "1.0"
      },
      "userHistory": {
        "connection": [ "" ],
        "removeFreqMs": "10000"
      },
      "userManager": {
        "connection": [ "UserProcessor:2" ],
        "maxUsers": "1",
        "userAwaySec": "15.0",
        "userOverlap": "0.2",
        "templateScale": "0.3"
      },
      "userProcessor": {
        "connection": [ "UserHistory:1", "Visualizer:2" ],
        "headPose": {
          "estimateReprojection": "FALSE",
          "faceBoxOffset": "5.0"
        },
        "shapeModel": {
          "clamp": "3.0",
          "conFile": "face.con",
          "failureCheck": "FALSE",
          "fTol": "0.01",
          "nIter": "5",
          "trackerFile": "face.tracker",
          "triFile": "face.tri",
          "winDetection": "11,9,7",
          "winTracking": "7"
        },
        "shapeNorm": {
          "maxCount": "1000",
          "epsilon": "1e-6"
        }
      },
      "Visualizer": {
        "connection": [ "Last:1" ]
      }
    }
  }
}